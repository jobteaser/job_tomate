#!/usr/bin/env ruby

# Usage
#
#   bin/run_workflow toggl_reports_to_jira_worklogs
#
require_relative '../config/boot'

workflow_name = ARGV[0]
require File.expand_path("../../lib/job_tomate/workflows/#{workflow_name}", __FILE__)
"JobTomate::Workflows::#{workflow_name.camelize}".constantize.run(*ARGV[1..-1])
